<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="OscarSandbox" verbose="1" >

    <test name="RegistrationAndLoginPositive" >
        <classes>
            <class name="com.telran.oscarLatestVersion.tests.user.RegistrationTests">
                <methods>
                    <exclude name=".*Negative.*"></exclude>
                </methods>
            </class>

            <class name="com.telran.oscarLatestVersion.tests.user.LoginTests">
                <methods>
                    <exclude name=".*Negative.*"></exclude>
                </methods>
            </class>
        </classes>

    </test>
</suite>